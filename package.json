{
  "name": "blockchain-dev",
  "version": "1.0.0",
  "description": "a project for learn blockchain",
  "main": "index.js",
  "scripts": {
    "install-truffle": "npm install --save-dev truffle",
    "install-openzeppelin": "npm install --save-dev @openzeppelin/contracts",
    "install-ganache": "npm install --save-dev ganache-cli",
    "install-dev-tools": "npm install truffle @openzeppelin/contracts ganache-cli --save-dev",
    "init": "npx truffle init",
    "compile": "npx truffle compile",
    "startup-blockchain": "npx ganache-cli --deterministic",
    "deploy": "npx truffle migrate --network development",
    "console": "npx truffle console --network development"
  },
